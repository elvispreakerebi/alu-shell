#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process by using the kill command

# Get the PID of the 4-to_infinity_and_beyond process
PID=$(pgrep -f 4-to_infinity_and_beyond)

# Check if the PID was found
if [[ -n "$PID" ]]; then
  # Kill the process
  kill "$PID"
  echo "Process 4-to_infinity_and_beyond (PID: $PID) has been stopped."
else
  echo "Process 4-to_infinity_and_beyond not found."
fi
